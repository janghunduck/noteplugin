unit menuCmdID;

interface

const
  IDM  =  40000;
  IDM_FILE  =  (IDM + 1000);
  IDM_FILE_NEW   =  (IDM_FILE + 1);
  IDM_FILE_OPEN  =  (IDM_FILE + 2);



// 0  New
// 1  Open...
// 2  Open Containing Folder
// 3  Open Folder as Workspace
// 4  Open in Default Viewer
// 5  Reload from Disk
// 6  Save
// 7  Save As...
// 8  Save a Copy As...
// 9  Save All
//10  Rename...
//11  Close
//12  Close All
//13  Close Multiple Documents
//14  Move to Recycle Bin
//15  --------
//16  Load Session...
//17  Save Session...
//18  --------
//19  Print...
//20  Print Now
//21  --------
//22  Exit
  
implementation

end.
